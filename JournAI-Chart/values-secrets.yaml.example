secrets:
  # Existing secrets
  jwtSecret: ""  # Keep your existing JWT secret
  geminiApiKey: ""  # Keep your existing Gemini API key
  openaiApiKey: ""  # Keep your existing OpenAI API key
  
  # Database credentials
  db:
    host: ""  # Will be set to your RDS endpoint (from Terraform output)
    port: "5432"  # Default PostgreSQL port
    name: ""  # Match this with your Terraform db_name
    username: ""  # Set to your database username (from Terraform)
    password: ""  # Set to your database password (from Terraform)
  
  # AWS credentials
  aws:
    accessKeyId: ""  # Your AWS access key ID
    secretAccessKey: ""  # Your AWS secret access key
    region: "us-east-1"  # Or your preferred region
    s3Bucket: ""  # Your S3 bucket name (from Terraform output)